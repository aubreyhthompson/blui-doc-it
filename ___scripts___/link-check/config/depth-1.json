{
    "ignorePatterns": [
        {"pattern": "/style/iconography"},
        {"pattern": "/style/color"},
        {"pattern": "/resources"},
        {"pattern": "/release-notes"},
        {"pattern": "^..\/images"}
    ],
    "replacementPatterns": [
        {
            "pattern": "^..\/images",
            "replacement": "../../../public/images"
        },
        {
            "pattern": "^\/patterns$",
            "replacement": "../patterns/home.mdx"
        },
        {
            "pattern": "^\/([A-Za-z-\/]+)",
            "replacement": "../$1.mdx"
        }
    ],
    "httpHeaders": [
    ]
}